  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "i": "^0.3.6",
    "material-ui": "^0.20.2",
    "material-ui-search-bar": "^0.4.2",
    "node-sass-chokidar": "^1.3.3",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-accessible-accordion": "^2.4.4",
    "react-d3-radar": "^1.0.0-rc3",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-light-accordion": "^0.1.4",
    "react-radar-chart": "^1.0.21",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-star-rating-component": "^1.4.1",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/main.scss -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/main.scss src/styles/uncomp -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000"
}
